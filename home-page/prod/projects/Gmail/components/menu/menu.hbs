                </div>
        </div>

        <div class="main-box important-wrap">

                <div class="important-btn">
                </div>

                <div class="info">important
                </div>
                
                <div class="num">
                </div>
        </div>

        {{#each this}}
            <div class="main-box label {{this.name}}" data-id={{this.id}}>

                <div class="important-btn">
                </div>

                <div class="info">{{this.name}}
                </div>
                
                <div class="num">
                </div>
            </div>
        {{/each}}
    </div>

