
<div class="inside-menu">

    <div class="main-box-wrap menu-type">

        <div class="primary-menu">

            <div class="primary-panle">

                <div class="primary-svg">
                </div>
                
                <div class="email-primary">Primary
                </div>

            </div>

        </div>

        <div class="social-menu">

            <div class="social-panle">

                <div class="social-svg">
                </div>
                
                <div class="email-social">Social
                </div>

            </div>

        </div>

        <div class="promotions-menu">

            <div class="promotions-panle">

                <div class="promotions-svg">
