<div class="header">

    <div class="text">הוספת איש קשר חסום
    </div>  

    <svg viewBox="0 0 24 24" width="24" height="24" class="close-btn">
        <path fill="currentColor" d="M19.1 17.2l-5.3-5.3 5.3-5.3-1.8-1.8-5.3 5.4-5.3-5.3-1.8 1.7 5.3 5.3-5.3 5.3L6.7 19l5.3-5.3 5.3 5.3 1.8-1.8z">

    </path>
    </svg>

</div>

<div class="list">

    {{#each this.contacts}}
        <div class="item" data-id={{this.id}}>

            <div class="info-wrap">

                <div class="name">{{this.name}}
                </div>

                <div class="title">{{this.title}}
                </div>

            </div>

            <div class="pic-wrap">

                <img class="pic" src="{{this.svg}}">

            </div>


        </div>
    {{/each}} 

</div>