<div class="group-warp" data-id={{group.id}}>
    <div class="group-header">
        <div class="bottom-arrow-btn-wrap">
            <svg class="feather bottom-arrow-btn" viewBox="0 0 24 24" width="24" height="24" stroke="currentColor" stroke-width="3" fill="none" stroke-linecap="round" stroke-linejoin="round"><polyline points="6 9 12 15 18 9"></polyline></svg>
        </div>
        <div class="group-name-wrapper">
            <div class="group-name-wrap">
                <input type="text" value="{{group.name}}" class="group-name">
            </div>
        </div>
        <div class="features-items-row">
            <div class="sub-item-wrap features-item">
                <div class="title">Subitems
                </div>
            </div>
            <div class="status-item-wrap features-item">
                <div class="title">Status
                </div>
            </div>
            <div class="members-item-wrap features-item">
                <div class="title">Person
                </div>
            </div>
            <div class="date-item-wrap features-item">
                <div class="title">Date
                </div>
            </div>
            <div class="timeline-items-wrap features-item">
                <div class="title">Timeline
                </div>
            </div>
            <div class="progress-tracking-item-wrap features-item">
                <div class="title">Progress Tracking
                </div>
            </div>
            <div class="add-col-btn-wrap">
                <svg viewBox="0 0 24 24" width="24" height="24" stroke="currentColor" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round" class="feather add-col-btn"><line x1="12" y1="5" x2="12" y2="19"></line><line x1="5" y1="12" x2="19" y2="12"></line></svg>
            </div>
        </div>
    </div>
    <div class="group-settings-dialog hide">
        <div class="inside">
            <div class="grou-settings-item duplicate">
                <svg viewBox="0 0 20 20" fill="currentColor" width="16" height="16" aria-hidden="true" aria-label="Duplicate" class="group-settings-icon"><path d="M7.82576 3.7273C7.82576 3.58922 7.93769 3.47729 8.07576 3.47729H9.76937H13.1567C13.2184 3.47729 13.278 3.50016 13.3239 3.54147L15.94 5.89592C15.9927 5.94334 16.0227 6.01088 16.0227 6.08175V13.3637C16.0227 13.5017 15.9108 13.6137 15.7727 13.6137H8.07576C7.93769 13.6137 7.82576 13.5017 7.82576 13.3637V3.7273ZM8.07576 1.97729C7.10926 1.97729 6.32576 2.7608 6.32576 3.7273V4.88639H5.16667C4.20017 4.88639 3.41667 5.66989 3.41667 6.63639V16.2727C3.41667 17.2392 4.20018 18.0228 5.16667 18.0228H11.8939C12.8604 18.0228 13.6439 17.2392 13.6439 16.2727V15.1137H15.7727C16.7392 15.1137 17.5227 14.3302 17.5227 13.3637V6.08175C17.5227 5.58565 17.3122 5.11286 16.9434 4.78098L14.3274 2.42653C14.006 2.13732 13.589 1.97729 13.1567 1.97729H9.76937H8.07576ZM12.1439 15.1137H8.07576C7.10927 15.1137 6.32576 14.3302 6.32576 13.3637V6.38639H5.16667C5.0286 6.38639 4.91667 6.49831 4.91667 6.63639V16.2727C4.91667 16.4108 5.0286 16.5227 5.16667 16.5227H11.8939C12.032 16.5227 12.1439 16.4108 12.1439 16.2727V15.1137Z" fill="currentColor" fill-rule="evenodd" clip-rule="evenodd"></path></svg>
                <div class="group-setting-text">Duplicate
                </div>
            </div>
            <div class="grou-settings-item delete">
                <svg viewBox="0 0 20 20" fill="currentColor" width="16" height="16" aria-hidden="true" aria-label="Delete" class="group-settings-icon"><path d="M8.30035 1.86462C7.77994 1.86462 7.29477 2.08976 6.94732 2.46719C6.60179 2.84253 6.41724 3.33927 6.41724 3.84552V4.32642H4.901H2.63477C2.22055 4.32642 1.88477 4.6622 1.88477 5.07642C1.88477 5.49063 2.22055 5.82642 2.63477 5.82642H4.151V16.1545C4.151 16.6608 4.33556 17.1575 4.68109 17.5328C5.02853 17.9103 5.51371 18.1354 6.03411 18.1354H13.9659C14.4863 18.1354 14.9715 17.9103 15.3189 17.5328C15.6645 17.1575 15.849 16.6608 15.849 16.1545V5.82642H17.3652C17.7794 5.82642 18.1152 5.49063 18.1152 5.07642C18.1152 4.6622 17.7794 4.32642 17.3652 4.32642H15.099H13.5828V3.84552C13.5828 3.33927 13.3982 2.84253 13.0527 2.46719C12.7053 2.08976 12.2201 1.86462 11.6997 1.86462H8.30035ZM7.16447 5.82642C7.16539 5.82642 7.16631 5.82642 7.16724 5.82642H12.8328C12.8337 5.82642 12.8346 5.82642 12.8356 5.82642H14.349V16.1545C14.349 16.3012 14.2948 16.4306 14.2153 16.5169C14.1378 16.6012 14.0465 16.6354 13.9659 16.6354H6.03411C5.95348 16.6354 5.86223 16.6012 5.78468 16.5169C5.7052 16.4306 5.651 16.3012 5.651 16.1545V5.82642H7.16447ZM12.0828 4.32642V3.84552C12.0828 3.69887 12.0286 3.56943 11.9491 3.4831C11.8716 3.39886 11.7803 3.36462 11.6997 3.36462H8.30035C8.21972 3.36462 8.12847 3.39886 8.05091 3.4831C7.97144 3.56943 7.91724 3.69887 7.91724 3.84552V4.32642L12.0828 4.32642Z" fill="currentColor" fill-rule="evenodd" clip-rule="evenodd"></path></svg>
                <div class="group-setting-text">Delete
                </div>
            </div>
            <div class="grou-settings-item add-group">
                <svg viewBox="0 0 24 24" width="24" height="24" stroke="currentColor" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round" class="group-settings-icon"><line x1="12" y1="5" x2="12" y2="19"></line><line x1="5" y1="12" x2="19" y2="12"></line></svg> 
                <div class="group-setting-text">Add group
                </div>
            </div>
        </div>
    </div>
    <div class="item-list" data-id={{group.id}}>
    </div>
    <div class="add-item-btn-warp">
        <div class="item-info">
            <div class="left-line">
            </div>
            <input type="text" class="add-item-name" placeholder="+ Add item">

            <div class="add-item-btn hide disable">Add
            </div>
        </div>
    </div>      
    <div class="total-items-features {{#if isItemListEmpty}}hide{{/if}}">
        <div class="status-warp">
            <div class="status-color-row">
                <div class="status-color working" style="width: {{totalStatusWidthObj.workingWidth}}%;">
                </div>
                <div class="status-color stack" style="width: {{totalStatusWidthObj.stackWidth}}%;">
                </div>
                <div class="status-color done" style="width: {{totalStatusWidthObj.doneWidth}}%;">
                </div>
                <div class="status-color none" style="width: {{totalStatusWidthObj.noneWidth}}%;">
                </div>
            </div>
        </div>
        <div class="total-progress-tracking-item-wrapper">
            <div class="total-progress-tracking-item-wrap">
                <div class="total-progress-warp">
                    <div class="total-progress-row" style="width: {{totalProgress}};">
                    </div>
                </div>
                <div class="total-progress-percent" >{{totalProgress}}
                </div>
            </div>
        </div>
    </div>
</div>