<div class="item level-{{level}} {{task.name}}" data-id={{task.id}}>
    {{#if isViewMode}}
        <a href="#/task/view/{{task.id}}" class="content border-none {{#if task.isfinished}}finished{{/if}}">
            <div type="button" class="finish-checkbox {{this.priorityColor}}">
                
                <svg xmlns="http://www.w3.org/2000/svg" class="feather finish-checkbox-check" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
                <polyline points="20 6 9 17 4 12"/>
                </svg>

            </div>  
            <div class="info-wrap">

                <div class="name">{{task.name}}
                </div>

                <input type="text" class="discription" placeholder="Description" value="{{task.title}}">

                <div class="item-info-tag top">

                    <div class="date-picker-btn set-potions-btn date-wrap">

                        <div class="inside-date-picker-btn">

                            <svg class="calender-btn" data-svgs-path="sm1/calendar_small.svg" xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 16 16">
                        <path fill="currentColor" fill-rule="nonzero" d="M12 2a2 2 0 0 1 2 2v8a2 2 0 0 1-2 2H4a2 2 0 0 1-2-2V4a2 2 0 0 1 2-2h8zm0 1H4a1 1 0 0 0-1 1v8a1 1 0 0 0 1 1h8a1 1 0 0 0 1-1V4a1 1 0 0 0-1-1zm-1.25 7a.75.75 0 1 1 0 1.5.75.75 0 0 1 0-1.5zm.75-5a.5.5 0 1 1 0 1h-7a.5.5 0 0 1 0-1h7z">

                        </path>
                            </svg>

                            <div class="date-input">{{sendTime}}
                            </div>

                            <div class="label-date-input">
                            </div>

                        </div>
                    </div>

                    <div class="date-picker-dialog hide">
                    </div>

                    <div class="label-tag-wrap">
                    {{#each this.labelsNames}}
                        <div class='label-wrap'>
                            <div class='label-name'>{{this.name}}
                            </div>
                        </div>
                    {{/each}}
                    </div>

                </div>
            </div>
        </a>
    {{else}}
        <a href="#/task/view/{{task.id}}" class="content {{#if task.isfinished}}finished{{/if}}">
        {{#if isTaskHaveChildren}}
            <svg class="feather feather-down toggle-hide-btn" xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
            <polyline points="6 9 12 15 18 9"/>
            </svg>
            <svg class="feather feather-right hide toggle-hide-btn" xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
            <polyline points="9 18 15 12 9 6"/>
            </svg>
        {{/if}}
            <div type="button" class="finish-checkbox {{this.priorityColor}}">
                
                <svg xmlns="http://www.w3.org/2000/svg" class="feather finish-checkbox-check" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
                <polyline points="20 6 9 17 4 12"/>
                </svg>

            </div>
            <div class="info-wrap">

                <div class="name">{{task.name}}
                </div>

                <div class="item-info-tag">
                    <div type="button" class="date-wrap set-potions-btn features-btn">

                            <svg class="date-icon" data-svgs-path="sm1/calendar_small.svg" xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 16 16">
                            <path fill="currentColor" fill-rule="nonzero" d="M12 2a2 2 0 0 1 2 2v8a2 2 0 0 1-2 2H4a2 2 0 0 1-2-2V4a2 2 0 0 1 2-2h8zm0 1H4a1 1 0 0 0-1 1v8a1 1 0 0 0 1 1h8a1 1 0 0 0 1-1V4a1 1 0 0 0-1-1zm-1.25 7a.75.75 0 1 1 0 1.5.75.75 0 0 1 0-1.5zm.75-5a.5.5 0 1 1 0 1h-7a.5.5 0 0 1 0-1h7z">

                            </path>
                            </svg>

                            <div class="text date-input">{{dateDiff}}
                            </div>

                            <div class="text label-date-input">
                            </div>

                    </div>
                    <div class="date-picker-dialog hide">
                    </div>
                    <div class="label-tag-wrap">
                        {{#each this.labelsNames}}
                            <div class="label-item-wrap">
                                <svg xmlns="http://www.w3.org/2000/svg" class="label-icon" width="12" height="12" viewBox="0 0 12 12">
                            <path d="M9.357 1C10.264 1 11 1.736 11 2.643V6.07c0 .436-.173.854-.481 1.162L7.232 10.52a1.643 1.643 0 01-2.323 0L1.48 7.09c-.64-.64-.64-1.68.001-2.322L4.768 1.48C5.076 1.173 5.494 1 5.93 1h3.427zm-.07.91H5.93a.805.805 0 00-.569.235L2.145 5.362a.805.805 0 000 1.138L5.5 9.855a.805.805 0 001.138 0l3.217-3.217a.805.805 0 00.236-.569V2.713a.804.804 0 00-.804-.804zM7.364 3.726a.91.91 0 110 1.818.91.91 0 010-1.818z" fill="currentColor" fill-rule="evenodd">

                        </path>
                                </svg>
                                <div class="label">{{this.name}}
                                </div>
                            </div>
                        {{/each}}
                    </div>
                </div>
            </div>
            <div class="items-wrap">

                <svg width="24" height="24" class="edit-btn">
                    <g fill="none" fill-rule="evenodd">
                        <path fill="currentColor" d="M9.5 19h10a.5.5 0 110 1h-10a.5.5 0 110-1z">

                        </path>
                        <path stroke="currentColor" d="M4.42 16.03a1.5 1.5 0 00-.43.9l-.22 2.02a.5.5 0 00.55.55l2.02-.21a1.5 1.5 0 00.9-.44L18.7 7.4a1.5 1.5 0 000-2.12l-.7-.7a1.5 1.5 0 00-2.13 0L4.42 16.02z">

                </path>
                </g>
                </svg>
                <div class="settings-btn-wrap">
                    <svg class="settings-btn" width="15" height="3">
                        <path d="M1.5 3a1.5 1.5 0 1 1 0-3 1.5 1.5 0 0 1 0 3zm6 0a1.5 1.5 0 1 1 0-3 1.5 1.5 0 0 1 0 3zm6 0a1.5 1.5 0 1 1 0-3 1.5 1.5 0 0 1 0 3z" fill="currentColor" fill-rule="evenodd">

                    </path>
                    </svg>
                </div>
                <div class="settings-menu hide">
                    <div class="popper">
                    </div>
                    <div class="inside">
                        <div class="delete-item settings-item">

                            <svg class="delete-icon settings-icon" xmlns="http://www.w3.org/2000/svg" width="24" height="24"><g fill="none" fill-rule="evenodd"><path d="M0 0h24v24H0z"></path><rect width="14" height="1" x="5" y="6" fill="currentColor" rx=".5"></rect><path fill="currentColor" d="M10 9h1v8h-1V9zm3 0h1v8h-1V9z"></path><path stroke="currentColor" d="M17.5 6.5h-11V18A1.5 1.5 0 0 0 8 19.5h8a1.5 1.5 0 0 0 1.5-1.5V6.5zm-9 0h7V5A1.5 1.5 0 0 0 14 3.5h-4A1.5 1.5 0 0 0 8.5 5v1.5z"></path></g></svg>

                            <div class="settings-text">Delete task
                            </div>
                        </div>
                        <div class="duplicate-item settings-item">

                        <svg class="settings-icon duplicate-icon" width="24" height="24">
                            <g fill="none">
                            <path fill="currentColor" d="M11 13h2.5c.3 0 .5.2.5.5s-.2.5-.5.5H11v2.5a.5.5 0 0 1-.5.5.5.5 0 0 1-.5-.5V14H7.5a.5.5 0 0 1-.5-.5c0-.3.2-.5.5-.5H10v-2.5c0-.3.2-.5.5-.5s.5.2.5.5V13z">

                            </path>
                            <rect width="12" height="12" x="4.5" y="7.5" stroke="currentColor" rx="2">

                            </rect>
                            <path fill="currentColor" d="M19 16.7V6a1 1 0 0 0-1-1H7.3c.2-.3.4-.6.7-.7.3-.2.7-.3 1.6-.3h7.8c1 0 1.3 0 1.6.3.3.1.6.4.7.7.2.3.3.7.3 1.6v7.8c0 1 0 1.3-.3 1.6-.1.3-.4.5-.7.7zm-1.6.3H9.6h7.8z">

                            </path>
                            </g>
                        </svg>

                        <div class="settings-text">Duplicate
                        </div>

                        </div>
                    </div>
                </div>
            </div>

        </a>
    {{/if}}

    <div class="task-editor-wrap hide">
</div>