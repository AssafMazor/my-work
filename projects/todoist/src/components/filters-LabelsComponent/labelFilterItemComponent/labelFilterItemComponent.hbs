{{#if isLabelMode}}
    <div class="label-item" data-id={{label.id}}>

        <svg width="24" class="label-icon" height="24" viewBox="0 0 24 24">
            <path fill="currentColor" fill-rule="nonzero" d="M5.914 11.086l4.5-4.5A2 2 0 0 1 11.828 6H16a2 2 0 0 1 2 2v4.172a2 2 0 0 1-.586 1.414l-4.5 4.5a2 2 0 0 1-2.828 0l-4.172-4.172a2 2 0 0 1 0-2.828zM14 11a1 1 0 1 0 0-2 1 1 0 0 0 0 2z">

        </path>
        </svg>

        <div class="label-name">{{label.name}}
        </div>

        <div class="items-row">

            <svg class="heart-btn labels-action" xmlns="http://www.w3.org/2000/svg" width="24" height="24">
            <path fill="none" stroke="currentColor" d="M12 18.345c5.642-4.767 6.354-5.448 7.099-6.988a3.79 3.79 0 0 0 .401-1.69c0-2.307-1.772-4.167-3.944-4.167-1.252 0-2.407.62-3.15 1.657L12 7.725l-.407-.568C10.851 6.12 9.696 5.5 8.444 5.5 6.272 5.5 4.5 7.36 4.5 9.667c0 .582.13 1.13.401 1.69.745 1.54 1.457 2.221 7.099 6.988z">

            </path>
            </svg>

            <svg class="edit-btn labels-action" width="24" height="24">
                <g fill="none" fill-rule="evenodd">
                <path fill="currentColor" d="M9.5 19h10a.5.5 0 110 1h-10a.5.5 0 110-1z">

                </path>
                <path stroke="currentColor" d="M4.42 16.03a1.5 1.5 0 00-.43.9l-.22 2.02a.5.5 0 00.55.55l2.02-.21a1.5 1.5 0 00.9-.44L18.7 7.4a1.5 1.5 0 000-2.12l-.7-.7a1.5 1.5 0 00-2.13 0L4.42 16.02z">

            </path>
            </g>
            </svg>

            <svg class="more-settings-btn labels-action" width="15" height="3">
                <path d="M1.5 3a1.5 1.5 0 1 1 0-3 1.5 1.5 0 0 1 0 3zm6 0a1.5 1.5 0 1 1 0-3 1.5 1.5 0 0 1 0 3zm6 0a1.5 1.5 0 1 1 0-3 1.5 1.5 0 0 1 0 3z" fill="currentColor" fill-rule="evenodd">

            </path>
            </svg>

            <div class="setting-dialog hide">

                <div class="item edit-item">

                    <svg class="edit-icon icon" width="24" height="24">
                        <g fill="none" fill-rule="evenodd">
                        <path fill="currentColor" d="M9.5 19h10a.5.5 0 110 1h-10a.5.5 0 110-1z">

                        </path>
                        <path stroke="currentColor" d="M4.42 16.03a1.5 1.5 0 00-.43.9l-.22 2.02a.5.5 0 00.55.55l2.02-.21a1.5 1.5 0 00.9-.44L18.7 7.4a1.5 1.5 0 000-2.12l-.7-.7a1.5 1.5 0 00-2.13 0L4.42 16.02z">

                    </path>
                    </g>
                    </svg>

                    <div class="text">Edit label
                    </div>

                </div>

                <div class="item delete-item">

                    <svg class="delete-icon icon" xmlns="http://www.w3.org/2000/svg" width="24" height="24">
                    <g fill="none" fill-rule="evenodd">
                        <path d="M0 0h24v24H0z"></path><rect width="14" height="1" x="5" y="6" fill="currentColor" rx=".5"></rect><path fill="currentColor" d="M10 9h1v8h-1V9zm3 0h1v8h-1V9z"></path><path stroke="currentColor" d="M17.5 6.5h-11V18A1.5 1.5 0 0 0 8 19.5h8a1.5 1.5 0 0 0 1.5-1.5V6.5zm-9 0h7V5A1.5 1.5 0 0 0 14 3.5h-4A1.5 1.5 0 0 0 8.5 5v1.5z">

                    </path>
                    </g>
                    </svg>

                    <div class="text">Delete label
                    </div>

                </div>

                <div class="item favorite-item">

                    <svg class="favorite-icon icon" xmlns="http://www.w3.org/2000/svg" width="24" height="24">
                    <path fill="none" stroke="currentColor" d="M12 18.345c5.642-4.767 6.354-5.448 7.099-6.988a3.79 3.79 0 0 0 .401-1.69c0-2.307-1.772-4.167-3.944-4.167-1.252 0-2.407.62-3.15 1.657L12 7.725l-.407-.568C10.851 6.12 9.696 5.5 8.444 5.5 6.272 5.5 4.5 7.36 4.5 9.667c0 .582.13 1.13.401 1.69.745 1.54 1.457 2.221 7.099 6.988z">

                    </path>
                    </svg>

                    <div class="text">Add to favorites
                    </div>

                </div>

            </div>
        </div>
    </div>  
