<div class="label-item" data-id={{label.id}}>

    {{#if label.isAttache}}
        <div class="square checked">
            <svg xmlns="http://www.w3.org/2000/svg" width="14" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-check">
            <polyline points="20 6 9 17 4 12">

            </polyline>
            </svg>
        </div> 
    {{else}}
        <div class="square">
            <svg xmlns="http://www.w3.org/2000/svg" width="14" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-check">
            <polyline points="20 6 9 17 4 12">

            </polyline>
            </svg>
        </div> 
    {{/if}}

    <div class="label-name">{{label.name}}
    </div>

</div>
